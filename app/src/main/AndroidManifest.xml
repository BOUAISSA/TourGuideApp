<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.macbookpro.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Restaurents" android:label="Algiers' Restaurents" />
        <activity android:name=".Hotels" android:label="Algiers' Hotels"/>
        <activity android:name=".Parks" android:label="Algiers' Parks"/>
        <activity android:name=".Hospitals" android:label="Algiers' Hospitals"></activity>
    </application>

</manifest>